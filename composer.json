{
    "name": "ahsanatiq/voucher-pool-api",
    "description": "Voucher API service based on slim framework",
    "keywords": ["voucher", "php72", "microframework", "rest", "phpunit", "tests"],
    "homepage": "http://github.com/ahsanatiq/voucher-pool-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahsan Atiq",
            "email": "me@ahsanatiq.info",
            "homepage": "https://www.ahsanatiq.info/"
        }
    ],
    "require": {
        "php": "~7.2.0",
        "slim/slim": "^3.1",
        "fzaninotto/faker": "^1.8",
        "illuminate/config": "^5.7.0",
        "illuminate/database": "^5.7.0",
        "illuminate/pagination": "^5.7",
        "illuminate/validation": "^5.7.0",
        "league/fractal": "^0.17.0",
        "monolog/monolog": "^1.24",
        "robmorgan/phinx": "^0.10.6",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "codeception/c3": "^2.4",
        "codeception/codeception": "^2.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
